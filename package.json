{
  "name": "rengine",
  "private": true,
  "type": "module",
  "packageManager": "bun@1.3.6",
  "scripts": {
    "qt": "cd adapters/python/app-qt && ./run_app.sh",
    "gd": "godot4 ./examples/godot/rw/project.godot",
    "gd:build": "cd examples/godot/rw && ./build_and_copy.sh",
    "pub": "bun run build && bun dler publish",
    "latest": "bun dler update && bun check",
    "build": "bun clean:build && bun check && bun dler build --loggerClearInternals",
    "pub:minor": "bun pub --bump minor",
    "turbo:build": "turbo run build",
    "typecheck": "turbo run typecheck",
    "examples:generate": "turbo run generate --filter='@reliverse/examples-rempts-*'",
    "examples:typecheck": "turbo run typecheck --filter='@reliverse/examples-rempts-*'",
    "examples:build": "turbo run build --filter='@reliverse/examples-rempts-*'",
    "clean-ts": "bun scripts/clean.ts",
    "fresh": "bun clean && bun i",
    "clean": "./clean.sh",
    "clean:build": "bun dler clean --presets build --force",
    "test": "bun test",
    "dev": "bun scripts/dev.ts",
    "release": "bun scripts/release.ts",
    "lint": "biome lint --fix --unsafe",
    "format": "biome format --fix",
    "unused": "bun dler unused --ignore @reliverse/dler",
    "check": "bun dler tsc && bun format && bun lint",
    "test:blender": "cd adapters/python/blender && python3 tests/test_*.py",
    "test:app-qt": "cd adapters/python/app-qt && python3 tests/test_*.py",
    "test:python": "bun test:blender && bun test:app-qt"
  },
  "workspaces": {
    "packages": [
      "adapters/*"
    ],
    "catalog": {}
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@reliverse/dler": "^2.3.4",
    "turbo": "^2.7.5",
    "ultracite": "^7.0.11"
  }
}
