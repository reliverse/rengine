{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "dialog:allow-open",
    "dialog:allow-save",
    "fs:read-all",
    "fs:write-all",
    "core:window:default",
    "core:window:allow-start-dragging",
    "window-state:default"
  ]
}
