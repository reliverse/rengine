{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "rengine",
  "version": "0.2.0",
  "identifier": "com.reliverse.rengine",
  "build": {
    "beforeDevCommand": "bun run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Rengine",
        "width": 1600,
        "height": 900,
        "visible": false,
        "decorations": true,
        "transparent": false,
        "center": true,
        "resizable": true,
        "maximizable": true,
        "minimizable": true,
        "closable": true,
        "fullscreen": false,
        "skipTaskbar": false,
        "theme": null,
        "titleBarStyle": "Visible"
      }
    ],
    "security": {
      "csp": null
    },
    "macOSPrivateApi": false
  },
  "plugins": {
    "fs": {
      "requireLiteralLeadingDot": false
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "createUpdaterArtifacts": false,
    "windows": {
      "digestAlgorithm": "sha256",
      "timestampUrl": null,
      "tsp": false,
      "webviewInstallMode": {
        "type": "embedBootstrapper"
      }
    },
    "macOS": {
      "frameworks": [],
      "minimumSystemVersion": "10.15",
      "exceptionDomain": "",
      "signingIdentity": null,
      "providerShortName": null,
      "entitlements": null
    },
    "linux": {
      "deb": {
        "depends": []
      },
      "appimage": {
        "bundleMediaFramework": false
      }
    }
  }
}
